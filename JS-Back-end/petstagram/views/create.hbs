<!-- Starts Main Section -->
<main>
    <!-- Start Add Photo Section -->
    <div class="createPage">
        <h2>Add photo</h2>
        <!-- Start Add Photo Form -->
        <form action="/catalog/create" method="post">
            <p>
                <input type="text" name="name" value="{{photo.name}}" placeholder="Name">
            </p>

            <p>
                <input type="number" name="age" value="{{photo.age}}" placeholder="Age">
            </p>

            <p>
                <textarea type="text" name="description" placeholder="Description">{{photo.description}}</textarea>
            </p>

            <p>
                <input type="text" name="location" value="{{photo.location}}" placeholder="Location">
            </p>

            <p>
                <input type="text" name="imageUrl" value="{{photo.imageUrl}}" placeholder="Link to image">
            </p>
            <!-- Add Photo Button -->
            <button class="add-btn" type="submit">Add Photo</button>
        </form>
    </div>
</main>